{
    "name": "mis3085/laravel-cache-label",
    "description": "This is my package laravel-cache-label",
    "keywords": [
        "mis3085",
        "laravel",
        "laravel-cache-label"
    ],
    "homepage": "https://github.com/mis3085/laravel-cache-label",
    "license": "MIT",
    "authors": [
        {
            "name": "mis3085",
            "email": "17059877+mis3085@users.noreply.github.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "spatie/laravel-package-tools": "^1.9.2",
        "illuminate/contracts": "^8.0|^9.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.11",
        "nunomaduro/larastan": "^1.0",
        "orchestra/testbench": "^6.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Mis3085\\LaravelCacheLabel\\": "src",
            "Mis3085\\LaravelCacheLabel\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mis3085\\LaravelCacheLabel\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mis3085\\LaravelCacheLabel\\LaravelCacheLabelServiceProvider"
            ],
            "aliases": {
                "LaravelCacheLabel": "Mis3085\\LaravelCacheLabel\\Facades\\LaravelCacheLabel"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
